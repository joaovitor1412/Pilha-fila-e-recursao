#Faça um programa que leia 5 números e informe a soma e a média dos números.
número_1 = float(input("escreva número_1: "))
número_2 = float(input("escreva número_2: "))
número_3 = float(input("escreva número_3: "))
número_4 = float(input("escreva número_4: "))
número_5 = float(input("escreva número_5: "))

suma = (número_1 + número_2 + número_3 + número_4 + número_5 ) 
print(f"soma {suma}")


media = (número_1 + número_2 + número_3 + número_4 + número_5 ) / 5
print(f"media {media}")
